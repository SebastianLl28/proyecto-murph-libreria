doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Murph | #{pagina}
    link(rel="stylesheet", href="/css/dist/normalize.css")
    block styles
  body
    header.header
      .header__container
        a(href="#") 
          img.header__logo(src="/img/logo.png", alt="murph")
        nav.nav
          ul.nav__list
            li.nav__element
              a.nav__link(href="/app/dashboard") Inicio
            li.nav__element
              a.nav__link(href="/app/category") category
            li.nav__element
              a.nav__link(href="/app/product") Product
            li.nav__element
              a.nav__link(href="/app/users") Users
            li.nav__element
              a.nav__link(href="/app/customers") Customers
            li.nav__element
              a.nav__link(href="/app/supplier") Supplier
            li.nav__element
              a.nav__link(href="/app/profile") Profile
    main.main
      block contenido

    block scripts